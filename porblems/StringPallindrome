class Main{
    static boolean isPallinDrome(String str){
        int l = 0,r = str.length() -1;
        while(l<=r){
            if(str.charAt(l) != str.charAt(r)){
                return false;
            }
            l++;
            r--;
        }

        return true;
    }   

    public static void main(String[] args) {

        String s = "";
        System.out.print(isPallinDrome(s));        
    }
}