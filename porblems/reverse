class Main{

    static void Reverse(int arr[],int n){
            int l = 0 , r = n-1,temp;
            while(l<r){
                temp = arr[l];
                arr[l] = arr[r];
                arr[r] = temp; 
                l++;r--;
            }
    
    }

    public static void main(String[] args) {
        int arr[] = {1,2,3,4,5,6},n = arr.length;
        Reverse(arr, n);
        
        for(int i =0;i<n;i++){
            System.out.print(arr[i]+" ");
        }

    }
}